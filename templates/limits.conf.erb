# Puppet Name: <%= @title %>
#
<%
maxout = scope.lookupvar('::splunk::params::subsearch_maxout')
maxtime = scope.lookupvar('::splunk::params::subsearch_maxtime')
ttl = scope.lookupvar('::splunk::params::subsearch_ttl')
dispatch_dir_warning_size = scope.lookupvar('::splunk::params::dispatch_size')
-%>
[subsearch]
<% if maxout != :undef -%>maxout = <%= maxout %><% end -%>
<% if maxtime != :undef -%>maxtime = <%= maxtime %><% end -%>
<% if ttl != :undef -%>ttl = <%= ttl %><% end -%>

[search]
<% if dispatch_dir_warning_size != :undef -%>
dispatch_dir_warning_size = <%= dispatch_dir_warning_size %>
<% end -%>
