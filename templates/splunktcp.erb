<%
  caPath = scope['::splunk::params::caPath']
  certPath = scope['::splunk::params::certPath']
  cipherSuite = scope['::splunk::params::cipherSuite']
  sslVersions = scope['::splunk::params::sslVersions']
-%>
[SSL]
cipherSuite = <%= cipherSuite %>
requireClientCert=false
rootCA = $SPLUNK_HOME/<% caPath %>
serverCert = $SPLUNK_HOME/<% certPath %>
sslVersions = <%= sslVersions %>

[splunktcp://9997]
disabled=false

[splunktcp-ssl://9998]
disabled=false
