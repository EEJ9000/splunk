# Puppet Name: <%= @title %>
#
<%
enableSplunkWebSSL = scope['::splunk::params::webSSL']
privKeyPath = scope['::splunk::params::privKeyPath']
caCertPath = scope['::splunk::params::caCertPath']
cipherSuite = scope['::splunk::params::cipherSuite']
sslVersions = scope['::splunk::params::sslVersions']
splunkType = scope['::splunk::type']
-%>
[settings]
enableSplunkWebSSL = <%= enableSplunkWebSSL %>
allowSslRenegotiation = false
<% if privKeyPath == true -%>
privKeyPath =<%= privKeyPath %>
caCertPath = <%= caCertPath %><% end -%>
cipherSuite = <%= cipherSuite %>
sslVersions = <%= sslVersions %>
